{
  "manifest_version": 2,

  "name": "List of Lists Plugin",
  "description": "This stores multiple lists if links to all open at once.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
